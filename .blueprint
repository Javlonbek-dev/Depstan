created:
    - database/factories/ArticleFactory.php
    - database/factories/CategoryFactory.php
    - database/factories/TagFactory.php
    - database/factories/CommentFactory.php
    - database/migrations/2024_10_10_071206_create_articles_table.php
    - database/migrations/2024_10_10_071207_create_categories_table.php
    - database/migrations/2024_10_10_071208_create_tags_table.php
    - database/migrations/2024_10_10_071209_create_comments_table.php
    - database/migrations/2024_10_10_071210_create_article_tag_table.php
    - app/Models/Article.php
    - app/Models/Category.php
    - app/Models/Tag.php
    - app/Models/Comment.php
models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable' }
    Article: { title: string, content: text, author_id: 'unsignedInteger nullable', category_id: 'unsignedInteger nullable', status: string, featured_image: 'string nullable', published_at: 'timestamp nullable', deleted_at: softDeletes, timestamps: timestamps, relationships: { belongsTo: 'User, Category', belongsToMany: Tag } }
    Category: { name: 'string unique', timestamps: timestamps, relationships: { hasMany: Article } }
    Tag: { name: 'string unique', timestamps: timestamps, relationships: { belongsToMany: Article } }
    Comment: { article_id: 'unsignedInteger nullable', user_id: 'unsignedInteger nullable', content: text, status: string, timestamps: timestamps, relationships: { belongsTo: 'Article, User' } }
