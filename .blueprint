models:
    User: { name: string, email: string, email_verified_at: 'datetime nullable', password: string, remember_token: 'string nullable' }
    Article: { title: string, content: text, author_id: 'foreignId:user', category_id: 'foreignId:category', status: string, featured_image: 'string nullable', published_at: 'timestamp nullable', deleted_at: softDeletes, timestamps: timestamps, relationships: { belongsTo: 'author category', belongsToMany: Tag } }
    Category: { name: 'string unique', slug: 'string unique', timestamps: timestamps, relationships: { hasMany: Article } }
    Tag: { name: 'string unique', slug: 'string unique', timestamps: timestamps, relationships: { belongsToMany: Article } }
    Comment: { article_id: 'foreignId:article', user_id: 'foreignId:user nullable', content: text, status: string, timestamps: timestamps, relationships: { belongsTo: 'article user' } }
